{
  "name": "spectator-jest",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod --no-progress",
    "test": "ng test --watch",
    "lint": "ng lint",
    "test:ci": "ng test --ci --silent",
    "commit": "lint-staged && git-cz --no-verify"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.2.0",
    "@angular/common": "~10.2.0",
    "@angular/compiler": "~10.2.0",
    "@angular/core": "~10.2.0",
    "@angular/forms": "~10.2.0",
    "@angular/platform-browser": "~10.2.0",
    "@angular/platform-browser-dynamic": "~10.2.0",
    "@angular/router": "~10.2.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/jest": "^10.0.1",
    "@angular-devkit/build-angular": "~0.1002.0",
    "@angular/cli": "~10.2.0",
    "@angular/compiler-cli": "~10.2.0",
    "@ngneat/spectator": "^6.1.2",
    "@types/jest": "^26.0.18",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "commitizen": "^4.0.0",
    "cz-conventional-changelog": "^3.1.0",
    "husky": "^4.2.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.0.0",
    "ng-mocks": "^10.0.0",
    "prettier": "~2.0.5",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~4.0.2",
    "typescript-tslint-plugin": "^1.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,md,ts,html}": "prettier --write",
    "*.ts": "tslint -c tslint.json -p tsconfig.json --fix"
  },
  "commitizen": {
    "path": "cz-conventional-changelog"
  }
}
